{
  "name": "dravencms/discussion",
  "type": "dravencms-package",
  "description": "Discussion module for dravencms",
  "keywords": [
    "dravencms",
    "discussion"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "minetro/recaptcha": "@dev"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "discussion": "Dravencms\\Discussion\\DI\\DiscussionExtension"
        }
      },
      "scripts": {
        "post-install": "Dravencms\\Discussion\\Script\\PostInstall",
        "pre-uninstall": "Dravencms\\Discussion\\Script\\PreUninstall"
      }
    }
  }
}